{
    "extends": "eslint:recommended",
    "plugins": ["angular"],
    "env": {
        "browser": true,
        "jasmine": true
    },
    "globals": {
        "angular": true,
        "module": true,
        "inject": true
    },
    "rules": {
        "curly": ["error"],
        "eqeqeq": ["error"],
        "no-multi-spaces": ["error"],
        "eol-last": ["error"],
        "func-call-spacing": ["error"],
        "indent": ["error"],
        "no-multiple-empty-lines": ["error"],
        "no-tabs": ["error"],
        "no-trailing-spaces": ["error"],
        "space-before-function-paren": ["error"]
    }
}
